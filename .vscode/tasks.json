{
    "version": "2.0.0",
    "tasks": [
        {
          "label": "build",
          "command": "dotnet",
          "type": "process",
          "args": [
               "build",
               "${workspaceFolder}/MatchFinder-mn.sln",
               "/property:GenerateFullPaths=true",
               "/consoleloggerparameters:NoSummary;ForceNoAlign"
          ],
          "problemMatcher": "$msCompile"
     },
        {
          "label": "publish",
          "command": "dotnet",
          "type": "process",
          "args": [
               "publish",
               "${workspaceFolder}/MatchFinder-mn.sln",
               "/property:GenerateFullPaths=true",
               "/consoleloggerparameters:NoSummary;ForceNoAlign"
          ],
          "problemMatcher": "$msCompile"
     },
        {
          "label": "watch",
          "command": "dotnet",
          "type": "process",
          "args": [
               "watch",
               "run",
               "--project",
               "${workspaceFolder}/MatchFinder-mn.sln"
          ],
          "problemMatcher": "$msCompile"
     }
    ]
}